FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

field		6.24e-5;

internalField   uniform $field;

boundaryField
{
    aerofoil
    {
        type           fixedValue;
        value		uniform 0;
    }

    freestream
    {
        type            freestream;
        freestreamValue uniform $field;
    }

    frontAndBack
    {
        type            empty;
    }
}


